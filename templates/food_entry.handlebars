<link rel="stylesheet" href="http://raw.githubusercontent.com/sindresorhus/github-markdown-css/gh-pages/github-markdown.css">
<style>
    .markdown-body {
        min-width: 200px;
        max-width: 790px;
        margin: 0 auto;
        padding: 30px;
    }
</style>
<article class="markdown-body">

<h1>{{title}}</h1>

<ul id="knowledge-list">
{{#each documents}}
  <li>
    {{conclusion}}
    <ul>
      <li><a href="{{URL}}">Link to article</a></li>
      {{#if humanReadableCitation}}
        <li>{{humanReadableCitation}}</li>
      {{/if}}
    </ul>
  </li>
{{/each}}
</ul>

</article>
